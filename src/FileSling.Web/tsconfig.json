{
  "compileOnSave": true,
  "include": [
    "typescript/*"
  ],
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "strict": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "jsx": "preserve",

    "outDir": "wwwroot/js-client"
  }
}